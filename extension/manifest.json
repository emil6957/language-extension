{
    "name": "language extension",
    "version": "1.0.0",
    "manifest_version": 3,

    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },

    "action": {
        "default_popup": "index.html"
    },

    "permissions": [
        "contextMenus"
    ]
}